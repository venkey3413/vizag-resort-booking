{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "events:PutEvents"
            ],
            "Resource": [
                "arn:aws:events:*:*:event-bus/vizag-resort-events",
                "arn:aws:events:*:*:event-bus/default"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "events:CreateEventBus",
                "events:DescribeEventBus"
            ],
            "Resource": "arn:aws:events:*:*:event-bus/vizag-resort-events"
        }
    ]
}